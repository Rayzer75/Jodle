{{ > header }}
<header class="bar bar-nav">
	<h1 class="title">Contacts</h1>
</header>
<ul class="table-view">
	<li class="table-view-divider sucess">Contacts disponibles</li>
	{{#available_contacts}}
	<li class="table-view-cell media">
		<a class="navigate-right">
			<img class="media-object pull-left" src="http://placehold.it/42x42">
			<div class="media-body">
				{{ nickname }}
				<ul>
					<li><label>Nom : </label>{{ familyName }}</li>
					<li><label>Prénom : </label>{{ givenName }}</li>
					<li><label>Numéro de Téléphone : </label>{{ phoneNumber }}</li>
				</ul>
			</div>
		</a>
	</li>
	{{/available_contacts}}
	<li class="table-view-divider danger">Contacts non disponibles</li>
</ul>
{{ > footer }}